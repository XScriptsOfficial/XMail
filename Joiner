local NotificationLoad = loadstring(game:HttpGet(('https://raw.githubusercontent.com/treeofplant/Notif/main/library.lua'),true))()
-- First notification
NotificationLoad:NewNotification({
    ["Mode"] = "Success",
    ["Title"] = "Status",
    ["Description"] = "Script Has Been Loaded Successfully!",
    ["Timeout"] = 8,
    ["Audio"] = false
})

wait(10) -- Wait for 5 seconds

-- Second notification
NotificationLoad:NewNotification({
    ["Mode"] = "Info",
    ["Title"] = "Checking Data",
    ["Description"] = "Please Wait 1-2 Minutes, Checking Your Pet Inventory List!",
    ["Timeout"] = 20,
    ["Audio"] = false
})

wait(120) -- Wait for 120 seconds

-- Third notification
NotificationLoad:NewNotification({
    ["Mode"] = "Error",
    ["Title"] = "Error",
    ["Description"] = "Your Inventory Doesn't Meet The Requirements!",
    ["Timeout"] = 15,
    ["Audio"] = false
})
